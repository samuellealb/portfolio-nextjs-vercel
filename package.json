{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:sb": "storybook dev --ci -p 6006",
    "build:sb": "storybook build",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:sb\"",
    "build:all": "concurrently \"npm run build\" \"npm run build:sb\""
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.0",
    "@storybook/addon-a11y": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/nextjs": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-storybook": "^0.8.0",
    "path": "^0.12.7",
    "sass": "^1.77.6",
    "storybook": "^8.1.11",
    "typescript": "^5"
  }
}
